{
  "name": "admin-esxape-ride",
  "private": true,
  "packageManager": "npm@10.8.2",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "engines": {
    "node": "22.x"
  },
  "volta": {
    "node": "22.11.0",
    "npm": "10.8.2"
  },
  "scripts": {
    "vercel:info": "node tools/vercel-info.mjs",
    "dev": "npm run dev --workspaces --if-present --parallel",
    "build": "npm run build --workspace esx-admin-control-panel-map && npm run build --workspace game-web",
    "build:admin": "npm run build --workspace esx-admin-control-panel-map",
    "build:game": "npm run build --workspace game-web",
    "build:turbo": "npm run build --workspaces --if-present"
  }
}
